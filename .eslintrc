{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
  ],
  "plugins": [
    "import",
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "ecmaFeatures": {
    "defaultParams": true
  },
  "rules": {
    arrow-parens: 0,
    import/no-unresolved: [2, {commonjs: true, amd: true}],
    import/named: 2,
    comma-dangle: [
      2,
      "always-multiline"
    ],
    prefer-const: 2,
    react/prop-types: 0,
    no-var: 2,
    no-underscore-dangle: 0,
    react/jsx-filename-extension: [1, { "extensions": [".js", ".jsx"] }],
  }
}
