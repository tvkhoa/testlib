{
  "parser": "babel-eslint",
  "extends": "react-app",
  "plugins": [
    "import",
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "ecmaFeatures": {
    "defaultParams": true
  },
  "rules": {
    import/no-unresolved: [2, {commonjs: true, amd: true}],
    import/named: 2,
    import/namespace: 2,
    import/default: 2,
    import/export: 2,
  }
}
